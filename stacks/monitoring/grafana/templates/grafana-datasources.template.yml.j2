apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    url: {{ PROMETHEUS_URL }}
    editable: true
    
  - name: Loki
    type: loki
    access: proxy
    url: {{ LOKI_URL }}
    editable: true
 
  - name: alertmanager
    type: alertmanager
    access: proxy
    url: {{ ALERTMANAGER_URL }}
    jsonData:
      implementation: 'prometheus'
    editable: true


